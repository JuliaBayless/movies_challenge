{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "target": "es5",
      "lib": [
       "dom",
      "dom.iterable",
      "es2017"
    ],
      "module": "esnext",
      "jsx": "react-jsx",
      "outDir": "./dist/frontend",
      "strict": true,
      "noFallthroughCasesInSwitch": true,
      "isolatedModules": true,
      "moduleResolution": "node"
    },
    "include": ["src/**/*"]
  }